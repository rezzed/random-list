{
  "name": "random-list",
  "private": true,
  "version": "1.1.1",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && npm run test && vue-tsc --noEmit && vite build",
    "preview": "vite preview --host",
    "lint": "eslint --ext=js,ts,vue ./src",
    "format": "prettier \"**/*\" --write --ignore-unknown --ignore-path .gitignore",
    "test": "vitest",
    "test:verbose": "vitest --reporter verbose",
    "test:related": "vitest related --reporter verbose"
  },
  "dependencies": {
    "@mdi/js": "^6.6.96",
    "@vueuse/core": "^8.3.1",
    "pinia": "^2.0.13",
    "vue": "^3.2.32",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "bulma": "^0.9.3",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^19.0.0",
    "prettier": "2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.6",
    "vitest": "^0.10.0",
    "vue-tsc": "^0.34.11"
  }
}
